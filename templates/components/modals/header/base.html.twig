<div id="frgef--neo-page-builder-fixed-modal-header-action" class="frgef--neo-page-builder-fixed-modal" data-uuid="{{ uuid }}">
    <div class="frgef--neo-page-builder-fixed-modal-container">
        <div class="frgef--neo-page-builder-fixed-modal-header">
            <p class="frgef--neo-page-builder-fixed-modal-title">
                {% block title %}{% endblock %}
            </p>
            <div class="frgef--neo-page-builder-fixed-modal-close-wrapper">
                {% include '@!NeoPageBuilder/components/icons/headband/close.html.twig' %}
            </div>
        </div>
        <div class="frgef--neo-page-builder-fixed-modal-header-container">
            {% block content %}{% endblock %}
        </div>
    </div>
</div>
